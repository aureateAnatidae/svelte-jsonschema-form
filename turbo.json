{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalPassThroughEnv": [
    "PLAYWRIGHT_BROWSERS_PATH"
  ],
  "tasks": {
    "check": {
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "dependsOn": [
        "check"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    },
    "test": {},
    "preview": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "build"
      ]
    }
  }
}